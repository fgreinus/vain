{
    "name": "vainproject/vain",
    "description": "Project to build a laravel-driven content management system that's both, heavily extensible and easy to work with.",
    "type": "project",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "laravel/socialite": "~2.0",
        "laravelcollective/html": "5.1.*",
        "laravelrus/localized-carbon": "dev-master",
        "davejamesmiller/laravel-breadcrumbs": "3.*",
        "pingpong/modules": "2.1.*",
        "dowilcox/knp-menu-laravel": "dev-master",
        "baum/baum": "~1.1",
        "barryvdh/laravel-debugbar": "2.0.*",
        "filp/whoops": "1.1.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Vain\\": "src/Vain/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
